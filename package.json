{
  "name": "@lukso/design-system",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./lds-test": {
      "require": "./dist/lds-test.cjs",
      "import": "./dist/lds-test.js"
    },
    "./lds-test2": {
      "require": "./dist/lds-test2.cjs",
      "import": "./dist/lds-test2.js"
    }
  },
  "version": "1.0.1",
  "scripts": {
    "start": "vite dev --host",
    "dev": "vite dev --host",
    "build": "vite build",
    "lint": "eslint src --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test": "yarn build-storybook && concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\""
  },
  "devDependencies": {
    "@lit-labs/motion": "^1.0.3",
    "@storybook/addon-essentials": "^7.0.0-beta.21",
    "@storybook/addon-interactions": "^7.0.0-beta.21",
    "@storybook/addon-links": "^7.0.0-beta.21",
    "@storybook/blocks": "^7.0.0-beta.21",
    "@storybook/builder-vite": "^7.0.0-beta.21",
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.9.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/web-components-vite": "^7.0.0-beta.21",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "http-server": "^14.1.1",
    "lit": "^2.5.0",
    "lit-html": "^2.5.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "storybook": "^7.0.0-beta.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "wait-on": "^7.0.1"
  },
  "packageManager": "yarn@3.3.1"
