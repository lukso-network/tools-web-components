  "scripts": {
    "config-prepare": "rm -fr ./tools && tsc-bundle ./tsconfig.colorPalette.json --exportAs commonjs && tsc-bundle ./tsconfig.copyAssets.json --exportAs commonjs",
    "start": "yarn config-prepare && concurrently 'ts-node-esm ./vite.full.config.ts --mode=develop' 'yarn storybook' 'yarn analyze:watch'",
    "dev": "yarn start",
    "build": "rm -rf dist && yarn config-prepare && yarn analyze && ts-node-esm ./vite.full.config.ts --mode=production && yarn sass --verbose ./src/shared/styles/main.scss ./dist/styles/main.css && yarn build:list-exports",
    "build:list-exports": "node ./listExports.cjs",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:{eslint,css,fmt}'",
    "lint:eslint": "eslint src --fix",
    "lint:fmt": "prettier --check --loglevel=warn .",
    "lint:css": "stylelint \"**/*.{scss,css}\"",
    "lint:fmt:fix": "prettier --write .",
    "lint:types": "tsc --noEmit --project tsconfig.json",
    "analyze": "cem analyze --litelement --globs 'src/**/index.ts' --exclude 'src/**/*.stories.ts'",
    "analyze:watch": "cem analyze --litelement --globs 'src/**/index.ts' --exclude 'src/**/*.stories.ts' --watch",
    "storybook": "yarn config-prepare && storybook dev -p 6006",
    "build-storybook": "yarn config-prepare && storybook build",
    "test-storybook": "yarn config-prepare && test-storybook",
    "test": "yarn analyze && yarn config-prepare && yarn build-storybook && concurrently -k -s first -n 'SB,TEST' -c 'magenta,blue' 'http-server storybook-static --port 6007' 'wait-on -v tcp:127.0.0.1:6007 && yarn test-storybook --url http://127.0.0.1:6007'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "prettier --list-different",
      "eslint"
    ],
    "*.json": "prettier --list-different",
    "*.ya?ml": "prettier --list-different",
    "*.{scss,css,vue}": [
      "prettier --list-different",
      "stylelint"
    ]
  },
